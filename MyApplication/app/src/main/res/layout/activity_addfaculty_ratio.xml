<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightblue"
    tools:context=".Addfaculty_ratio">


    <RelativeLayout
        android:id="@+id/relativeLayout6"
        android:layout_width="353dp"
        android:layout_height="299dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.578">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="match_parent"
            android:layout_height="302dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="-2dp"
            android:layout_marginBottom="-1dp"
            app:srcCompat="@drawable/greybg" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_alignBottom="@+id/imageView9"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="256dp"
            android:text="  Faculty per student                                    "
            android:textColor="@color/black"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_alignBottom="@+id/imageView9"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="15dp"
            android:text="  No Faculty For Other Dept                     "
            android:textColor="@color/black"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:layout_alignBottom="@+id/imageView9"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="73dp"
            android:text="  No Faculty For CSE Dept                      "
            android:textColor="@color/black"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:layout_alignBottom="@+id/imageView9"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="195dp"
            android:text=" Total No Faculty                                   "
            android:textColor="@color/black"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:layout_alignBottom="@+id/imageView9"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="145dp"
            android:text="  Total No Students                                 "
            android:textColor="@color/black"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/textView83"
            android:layout_width="155dp"
            android:layout_height="37dp"
            android:layout_alignBottom="@+id/textView34"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="0dp"
            android:gravity="center"
            android:hint="Enter"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/textView84"
            android:layout_width="155dp"
            android:layout_height="37dp"
            android:layout_alignBottom="@+id/textView34"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="-57dp"
            android:gravity="center"
            android:hint="Enter"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/textView85"
            android:layout_width="155dp"
            android:layout_height="37dp"
            android:layout_alignBottom="@+id/textView34"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="-114dp"
            android:gravity="center"
            android:hint="Enter"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/textView86"
            android:layout_width="115dp"
            android:layout_height="37dp"
            android:layout_alignBottom="@+id/textView34"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="-1dp"
            android:layout_marginBottom="-181dp"
            android:gravity="center"
            android:hint="Enter"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/textView87"
            android:layout_width="115dp"
            android:layout_height="37dp"
            android:layout_alignBottom="@+id/textView34"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="-2dp"
            android:layout_marginBottom="-252dp"
            android:gravity="center"
            android:hint="Enter"
            android:textSize="15sp"

            />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="43dp"
        android:layout_height="37dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:onClick="moredetails"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.105"
        app:srcCompat="@drawable/chevronback" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:padding="10dp"
        android:text="         ADD FACULTY RATIO :"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.204" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/btn_shape"
        android:onClick="completed"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.933" />


</androidx.constraintlayout.widget.ConstraintLayout>